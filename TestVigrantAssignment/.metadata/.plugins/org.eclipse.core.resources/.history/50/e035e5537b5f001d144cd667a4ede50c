package TestVigrantAssignment;

import org.openqa.selenium.WebDriver;
import org.openqa.selenium.WebElement;
import org.openqa.selenium.support.FindBy;
import org.openqa.selenium.support.PageFactory;

public class POMfile {
	public POMfile(WebDriver driver1)
	{
		PageFactory.initElements(driver1, this);
	}
	@FindBy(xpath = "//a[text()='December 17, 2021 (United States)']")
	private WebElement getimdbdate;
	public  WebElement getimdbdate()
	{
		return getimdbdate;
	}
	@FindBy(xpath = "//li[@class='ipc-inline-list__item']/following::li[75]//following::li")
	private WebElement getimdborigin;
	public  WebElement getimdborigin()
	{
		return getimdborigin;
	}
	@FindBy(xpath = "//table[@class='infobox vevent']//tr[12]//td")
	private WebElement getwikidate;
	public  WebElement getwikidate()
	{
		return getwikidate;
	}
	@FindBy(xpath = "//a[text()='December 17, 2021 (United States)']")
	private WebElement getwikiorigin;
	public  WebElement getwikiorigin()
	{
		return getwikiorigin;
	}
}
